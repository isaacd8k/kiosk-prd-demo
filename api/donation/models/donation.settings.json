{
  "kind": "collectionType",
  "collectionName": "donations",
  "info": {
    "name": "Donation",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "location": {
      "model": "locations"
    },
    "name": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "paymentTypesAccepted": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "enumeration",
      "enum": [
        "cash",
        "credit"
      ],
      "required": true
    },
    "note": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "featImg": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "isWheelchairAccessible": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "boolean",
      "default": false,
      "required": true
    }
  }
}
