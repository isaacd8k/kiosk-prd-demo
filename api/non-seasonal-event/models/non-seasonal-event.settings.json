{
  "kind": "collectionType",
  "collectionName": "non_seasonal_events",
  "info": {
    "name": "Non-seasonal Event"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "startDate": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "date",
      "required": true
    },
    "languageCode": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "enumeration",
      "enum": [
        "EN",
        "ES",
        "PL",
        "IT",
        "KO"
      ],
      "required": true
    },
    "durationDays": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "integer",
      "required": true,
      "default": 1
    },
    "title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    }
  }
}
